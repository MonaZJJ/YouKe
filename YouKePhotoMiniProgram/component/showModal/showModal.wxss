.mask {
  width: 100%;
  height: 100%;
  position: fixed;
  background-color: #000;
  z-index: 222;
  top: 0;
  left: 0;
  opacity: 0.6;
}

.modal {
  position: fixed;
  left: 50%;
  top: 50%;
  transform: translate(-50%, -50%);
  z-index: 333;
  width: 600rpx;
  height: 680rpx;
  background: #fff;
  border-radius: 30rpx;
}

.scroll-view_modal {
  width: 100%;
  height: 526rpx;
  position: relative;
  z-index: 400;
  top: 0;
  left: 0;
  margin-top: 30rpx;
}

.submitBtn_modal {
  width: 470rpx;
  height: 110rpx;
  line-height: 110rpx;
  background: #383838;
  border-radius: 14rpx;
  color: #fff;
  font-size: 36rpx;
  position: fixed;
  z-index: 999;
  bottom: -55rpx;
  left: 65rpx;
}

.contactUs_bg {
  position: absolute;
  z-index: 350;
  top: 0;
  left: 0;
  height: 100%;
  width: 100%;
  border-radius: 30rpx;
}
