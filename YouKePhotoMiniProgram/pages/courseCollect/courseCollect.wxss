/* 教程收藏样式 */

.courseCollect {
  margin: 20rpx 20rpx 41rpx 20rpx;
}

.courseDesc {
  display: flex;
  flex-direction: row;
  align-items: center;
  justify-content: space-between;
}

.courseTime {
  width: 630rpx;
  font-size: 36rpx;
  color: #383838;
  font-weight: bold;
  overflow: hidden;
  white-space: nowrap;
  text-align: justify;
  text-overflow: ellipsis;
}

.courseNum {
  width: 630rpx;
  font-size: 24rpx;
  color: #ccc;
  overflow: hidden;
  white-space: nowrap;
  text-align: justify;
  text-overflow: ellipsis;
}

.collectSign {
  margin-right: 23rpx;
}
