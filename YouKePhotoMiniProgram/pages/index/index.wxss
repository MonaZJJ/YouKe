.welcome-box {
  padding: 0 30rpx;
  border-top: 1rpx solid transparent;
}

.welcome-box > text {
  display: block;
}

.welcome-box > text:first-child {
  line-height: 53rpx;
  font-size: 38rpx;
  color: #383838;
  margin-top: 27rpx;
}

.welcome-box .zanCom {
  line-height: 37rpx;
  font-size: 26rpx;
  color: #666;
  margin-top: 10rpx;
}

.active-box {
  width: 690rpx;
  height: 388rpx;
  border-radius: 14rpx;
  overflow: hidden;
  margin: 0 auto;
  margin-top: 30rpx;
  position: relative;
}

.joinBtn{
  position: absolute;
  z-index: 3;
  bottom: 20rpx;
  left:50%;
  margin-left:-70rpx;
  width: 140rpx;
  height: 52rpx;
  text-align: center;
  font-size: 24rpx;
  line-height: 52rpx;
  background: rgba(0,0,0,.6);
  border-radius: 26rpx;
  color: #fff;
}

.active-box > image {
  width: 100%;
  height: 388rpx;
}

.active-box > view.active_box_top {
  width: 650rpx;
  font-size: 20rpx;
  color: #fff;
  position: absolute;
  top: 20rpx;
  left: 20rpx;
}

.text-left {
  height: 38rpx;
  line-height: 38rpx;
  border-radius: 19rpx;
  background-color: #000;
  padding: 0 15rpx;
  float: left;
}

.text-left > text:nth-child(1) {
  width: 8rpx;
  height: 8rpx;
  border-radius: 4rpx;
  background-color: #fff;
  margin-top: 15rpx;
  float: left;
}

.text-left > text:nth-child(2) {
  margin-left: 15rpx;
  float: left;
}

.text-right {
  float: right;
}

.text-right text {
  line-height: 38rpx;
  float: left;
}

.text-right > view:nth-child(1) {
  float: left;
}

.text-right > view:nth-child(1) > image {
  width: 20rpx;
  height: 23rpx;
  margin-top: 7rpx;
  margin-right: 15rpx;
  float: left;
}

.text-right > view:nth-child(2) {
  float: left;
}

.text-right > view:nth-child(2) > image {
  width: 28rpx;
  height: 20rpx;
  margin: 9rpx 11rpx 0 26rpx;
  float: left;
}

.ranking-box {
  padding: 0 30rpx 30rpx 30rpx;
  margin-top: 30rpx;
  position: relative;
}

.ranking-title > text {
  line-height: 53rpx;
  font-size: 38rpx;
  color: #383838;
  display: inline-block;
  vertical-align: middle;
  position: relative;
}

.ranking-title > text::after {
  width: 120rpx;
  height: 20rpx;
  background:linear-gradient(90deg,rgba(255,233,208,1) 0%,rgba(244,241,228,1) 100%,rgba(255,233,208,1) 100%,rgba(244,241,228,1) 100%);
  position: absolute;
  left: 0;
  bottom: 6rpx;
  content: '';
  z-index: -99;
}
.ranking-title navigator{
  display: inline-block;
}
.ranking-title image {
  width: 30rpx;
  height: 30rpx;
  margin-left: 30rpx;
  vertical-align: middle;
}

.ranking-list {
  display: -webkit-flex;
  display: flex;
  -webkit-justify-content: space-between;
  justify-content: space-between;
  margin-top: 28rpx;
  padding: 0 23rpx;
}

.ranking-item > view {
  width: 190rpx;
  height: 190rpx;
  border-radius: 50%;
  /* border: 1rpx solid #979797; */
  box-sizing: border-box;
  position: relative;
}

.gouImg{
  width:38rpx;
  height:38rpx;
  position: absolute;
  z-index: 4;
  bottom: 10rpx;
  right: 15rpx;
}

.ranking-item > view > view {
  width: 178rpx;
  height: 178rpx;
  border-radius: 50%;
  /* border: 1rpx solid #979797; */
  box-sizing: border-box;
  position: absolute;
  top: 50%;
  left: 50%;
  transform: translate(-50%, -50%);
}

.ranking-item > view > view > view {
  width: 166rpx;
  height: 166rpx;
  border-radius: 50%;
  box-sizing: border-box;
  position: absolute;
  top: 50%;
  left: 50%;
  transform: translate(-50%, -50%);
  overflow: hidden;
}

.ranking-item > view > view > view > image {
  display: block;
  width: 100%;
  height: 100%;
}

.ranking-item > view > view > view > text {
  width: 58rpx;
  height: 58rpx;
  font-size: 14rpx;
  line-height: 28rpx;
  border-radius: 50%;
  color: #ffce79;
  background-color: rgba(0, 0, 0, 0.8);
  opacity: 0.6;
  text-align: center;
  padding-top: 30rpx;
  box-sizing: border-box;
  position: absolute;
  top: -30rpx;
  left: 50%;
  transform: translate(-50%, 0);
}

.ranking-item > text {
  width: 200rpx;
  overflow: hidden;
  text-overflow:ellipsis;
  white-space: nowrap;
  display: block;
  line-height: 37rpx;
  text-align: center;
  color: #383838;
  font-size: 26rpx;
  margin-top: 13rpx;
}

.left-circle {
  width: 59rpx;
  height: 140rpx;
  position: absolute;
  z-index: -1;
  left: 0;
  bottom: 0;
}

.right-circle {
  width: 40rpx;
  height: 122rpx;
  position: absolute;
  z-index: -1;
  right: 0;
  top: 41rpx;
}

.tab-box {
  display: -webkit-flex;
  display: flex;
  -webkit-justify-content: center;
  justify-content: center;
  height: 97rpx;
  background-color: #f8f8f8;
}

.tab-box > view {
  width: 186rpx;
  line-height: 40rpx;
  text-align: center;
  color: #a8a8a8;
  font-size: 28rpx;
  margin-top: 24rpx;
}

.tab-active {
  font-size: 36rpx !important;
  font-family: PingFangSC-Medium;
  font-weight: 500 !important;
  color: rgba(56, 56, 56, 1) !important;
  line-height: 50rpx;
  margin-top: 16rpx !important;
}

.tab-active::after {
  width: 11rpx;
  height: 11rpx;
  border-radius: 50%;
  background-color: #383838;
  content: '';
  display: block;
  margin: 0 auto;
  margin-top: 7rpx;
}

.conern-list {
  padding-top: 20rpx;
}

/* .concern-item {
  padding-bottom: 40rpx;
} */

.concern-top {
  display: -webkit-flex;
  display: flex;
  -webkit-justify-content: space-between;
  justify-content: space-between;
  padding-top: 20rpx;
}

.top-left > image {
  width: 85rpx;
  height: 85rpx;
  border-radius: 50%;
  float: left;
}

.top-left > text {
  max-width: 150rpx;
  height: 85rpx;
  line-height: 85rpx;
  font-size: 26rpx;
  color: #383838;
  float: left;
  margin-left: 10rpx;
  overflow: hidden;
}

.top-left > view {
  font-size: 24rpx;
  width: 110rpx;
  line-height: 36rpx;
  text-align: center;
  border-radius: 21rpx;
  border: 2rpx solid rgba(56, 56, 56, 1);
  float: left;
  margin-top: 25rpx;
  margin-left: 20rpx;
}

.top-left > view > text:nth-child(2) {
  color: #383838;
  margin-left: 5rpx;
}

.top-right {
  margin-top: 10rpx;
}

.top-right > view {
  height: 60rpx;
  color: #fff;
  background-color: rgba(0, 0, 0, 0.6);
  opacity: 0.4;
  border-radius: 30rpx;
  float: left;
  position: relative;
}

.top-right > view > text {
  display: block;
  line-height: 60rpx;
  font-size: 26rpx;
}

.top-right > view:nth-child(1) {
  width: 145rpx;
  padding-left: 81rpx;
  box-sizing: border-box;
}

.top-right > view:nth-child(2) {
  width: 150rpx;
  padding-left: 72rpx;
  box-sizing: border-box;
  margin-left: 15rpx;
}

.top-right > view:nth-child(1) > image {
  width: 37rpx;
  height: 33rpx;
  position: absolute;
  left: 22rpx;
  top: 13rpx;
}

.top-right > view:nth-child(2) > image {
  width: 40rpx;
  height: 37rpx;
  position: absolute;
  left: 20rpx;
  top: 13rpx;
}


.course-list {
  padding: 0 20rpx;
}

.course-item {
  margin-top: 20rpx;
  padding-bottom: 20rpx;
  position: relative;
}
.courseItem_play{
  position: absolute;
  z-index: 4;
  top: 250rpx;
  left: 23rpx;
  font-size: 24rpx;
  color: #fff;
  display: flex;
  align-items: center;
}
.courseItem_play image{
  width:30rpx;
  height:26rpx;
  margin-right: 10rpx;
}
.courseItem_duration{
  position: absolute;
  z-index: 4;
  top: 250rpx;
  right: 26rpx;
  font-size: 24rpx;
  color: #fff;
}
.course-item > image {
  width: 710rpx;
  height: 300rpx;
  border-radius:14rpx;
  display: block;
}

.course-item .courseItem_title {
  height: 50rpx;
  line-height: 50rpx;
  overflow: hidden;
  color: #383838;
  font-size: 36rpx;
  font-family: PingFangSC-Semibold;
  font-weight: 600;
  margin-top: 10rpx;
}

.course-item > text {
  height: 33rpx;
  line-height: 33rpx;
  overflow: hidden;
  color: #ccc;
  font-size: 24rpx;
  font-weight: 400;
  margin-top: 10rpx;
}

.twoOperation{
  position: fixed;
  bottom: 60rpx;
  right: 0rpx;
  /* transform: translate(0, 50%); */
  z-index: 20;
}
/* .twoOperation>view{
  width:120rpx;
  height:120rpx;
} */
.twoOperation image{
  width:120rpx;
  height:120rpx;
  vertical-align: top;
}

.unread {
display: inline-block;
width: 12rpx;
height: 12rpx;
border-radius: 50%;
background-color: rgb(223, 7, 7);
margin-bottom: 5rpx;
}
