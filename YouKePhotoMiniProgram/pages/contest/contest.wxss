.gainImg {
  background-color: #000;
  width: 690rpx;
  height: 240rpx;
  border-radius: 14rpx;
  position: relative;
  margin: 20rpx 30rpx;
}

.gainImg image {
  width: 690rpx;
  height: 240rpx;
  border-radius: 14rpx;
  -webkit-mask: -webkit-linear-gradient(rgba(0, 0, 0, 0.4), rgba(0, 0, 0, 0.4), rgba(0, 0, 0, 0.4), rgba(0, 0, 0, 0.4));
  mask: linear-gradient(rgba(0, 0, 0, 0.4), rgba(0, 0, 0, 0.4), rgba(0, 0, 0, 0.4), rgba(0, 0, 0, 0.4));
}

.title {
  position: relative;
  font-size: 48rpx;
  color: #fff;
  font-weight: 500;
  margin-top: -200rpx;
  text-align: center;
  z-index: 555;
}

.award {
  position: relative;
  color: #fff;
  font-size: 28rpx;
  font-weight: 400;
  margin-top: 20rpx;
  text-align: center;
  letter-spacing: 4rpx;
  z-index: 555;
}